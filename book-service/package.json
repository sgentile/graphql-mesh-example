{
    "name": "typgraphql-prisma-api",
    "version": "1.0.0",
    "description": "",
    "private": true,
    "scripts": {
        "build": "rm -rf dist/ && tsc --project tsconfig.json",
        "start:old": "ts-node --transpile-only src/index.ts",
        "predev": "prisma generate",
        "dev": "nodemon -w src --ext ts --exec 'ts-node -r tsconfig-paths/register --transpile-only src/index.ts'",
        "start": "node dist/index.js",
        "format": "prettier --write 'src/**/*.ts' && eslint --fix src/**/*.ts",
        "lint": "eslint src/**/*.ts --quiet",
        "seed": "ts-node --transpile-only ./prisma/seed.ts",
        "generate": "prisma generate",
        "migrate": "prisma migrate dev",
        "deploy": "prisma migrate deploy",
        "reset": "prisma migrate reset",
        "studio": "prisma studio",
        "prisma": "prisma format && npm run migrate && npm run seed",
        "test": "jest --passWithNoTests",
        "test:ci": "npm run test",
        "test:unit": "npm run test",
        "type-check": "tsc",
        "quality": "npm install --silent && npm run type-check && npm run lint && npm run test"
    },
    "dependencies": {
        "@prisma/client": "~3.1.1",
        "apollo-server": "^3.3.0",
        "apollo-server-core": "^3.5.0",
        "class-validator": "^0.13.1",
        "dotenv": "^10.0.0",
        "graphql": "^15.5.1",
        "graphql-fields": "^2.0.3",
        "graphql-scalars": "^1.12.0",
        "prisma": "~3.1.1",
        "reflect-metadata": "^0.1.13",
        "type-graphql": "^1.1.1",
        "type-graphql-dataloader": "^0.5.0",
        "typedi": "^0.10.0"
    },
    "devDependencies": {
        "@jest/types": "^27.2.4",
        "@types/graphql-fields": "^1.3.4",
        "@types/jest": "^27.0.2",
        "@types/node": "^16.11.7",
        "@types/pg": "^8.6.1",
        "@types/pluralize": "0.0.29",
        "@types/semver": "^7.3.8",
        "@typescript-eslint/eslint-plugin": "^5.3.1",
        "@typescript-eslint/parser": "^5.3.1",
        "concurrently": "^6.3.0",
        "directory-tree": "^3.0.0",
        "env-cmd": "^10.1.0",
        "eslint": "^7.8.1",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-prettier": "^3.1.4",
        "jest": "^27.2.4",
        "lint-staged": "^11.1.2",
        "nodemon": "^2.0.4",
        "prettier": "^2.4.1",
        "ts-jest": "~27.0.5",
        "ts-node": "^10.2.1",
        "tsconfig-paths": "^3.10.1",
        "typegraphql-prisma": "0.16.1",
        "typescript": "^4.4.4"
    }
}
